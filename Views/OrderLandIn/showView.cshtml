@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script type="text/javascript" src="/js/upload/plupload.full.min.js"></script>
<link href="/js/Extjs42/resources/css/ext-all-gray.css" rel="stylesheet" type="text/css" />
<script src="/js/Extjs42/bootstrap.js" type="text/javascript"></script>
<link href="~/css/bootstrap32/css/bootstrap.css" rel="stylesheet" />
<link href="/css/comme.css" rel="stylesheet" />
<link href="~/css/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
<script src="/js/pan.js" type="text/javascript"></script>
<script src="/js/jsyz.js"></script>
<script src="/js/commom_OrderView.js" type="text/javascript"></script>
<link href="/css/orderSearch.css" rel="stylesheet" />
<script src="/js/lib/jquery-1.7.1.js" type="text/javascript"></script>
<script src="/js/order_search.js" type="text/javascript"></script>
<script src="/js/common_utils.js" type="text/javascript"></script>
<script src="/js/echart/echarts-all.js"></script>
<script type="text/javascript">
    var orderId = getQueryString("OrderId");
    var OrderCode = getQueryString("OrderCode");
    var EntrustType = getQueryString("EntrustType");
    initViewField();
    Ext.onReady(function () {
        initViewControls();

        //表单控件布局
        var creatItems = [
            { layout: 'column', border: 42, border: 0, items: [label_baseinfo] },
            { layout: 'column', height: 42, margin: '5 0 0 0', border: 0, items: [field_CODE, field_ENTRUSTTYPE, field_REPWAY, field_CUSTOMDISTRICT, field_REPUNIT] },
            { layout: 'column', height: 42, border: 0, items: [field_DECLWAY, field_SUBMITUSERNAME, field_SUBMITTIME, field_SUBMITUSERPHONE, field_INSPUNITNAME] },
            { layout: 'column', height: 42, border: 0, items: [field_CREATEUSERNAME, field_CSNAME, field_CREATETIME, field_CSPHONE, field_STATUS] },
            { layout: 'column', border: 42, border: 0, items: [label_busiinfo] },
            { layout: 'column', height: 42, border: 0, items: [field_CUSNO, field_PORTCODE, field_BUSIUNIT, field_LADINGBILLNO, field_TOTALNO] },
            { layout: 'column', height: 42, border: 0, items: [field_quanpackage, field_weight, field_contractno, field_TRADEWAYCODES, field_MANIFEST] },
            { layout: 'column', height: 42, border: 0, items: [field_WOODPACKING, field_CLEARANCENO, field_LAWCONDITION, field_CONTAINERNO, field_DECLCARNO] },
            { layout: 'column', height: 42, border: 0, items: [field_ENTRUSTREQUEST] },
            field_ID
        ];

        //创建表单
        formpanel = Ext.create('Ext.form.Panel', {
            border: 0,
            minHeight: 350,
            fieldDefaults: {
                margin: '0 5 10 0',
                labelWidth: 80,
                columnWidth: .20,
                labelAlign: 'right',
                labelSeparator: '',
                msgTarget: 'under'
            },
            items: creatItems
        });

        //获取数据
        if (orderId) {
            formpanel.getForm().load({
                url: "/OrderLandIn/getForm",
                params: {
                    OrderId: orderId
                },
                success: function (form, action) {
                    initOrderList(action.result.data.CODE);
                }
            });
        }

        //报关报检单grid
        initOrdersGrid();

        //订单文件
        initFileList();

        //创建tab界面
        initTabPanel();

    });
</script>
<div class="container">
    <div>
        <div class="navposition">
            <span class="fr">当前位置：订单中心》陆进委托</span>
        </div>
    </div>
    <div id="orderTab">
      
    </div>
</div>

